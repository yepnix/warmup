pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- pongle by roe
-- thanks for playing a game!
--[[
this game combines the 
basics of pong with the pegs of
peggle, which randomly appear on
the field to add to the points
scored by whoever hits them.
]]

function _init()
	gamestate = 0
	paddlewidth = 2
	paddleheight = 8
	paddlespeed = 1
	ballsize = 2
	ballspeed = 3
end

function _update()
	if gamestate==0 then _update_menu()
	elseif gamestate==1 then _update_game()
	end
end

function _update_menu()
	if btnp(4) then gamestate=1 end
end

function _update_game()
	updatepaddles()
--	updateball()
--	updatepegs()
--	updatescore()
end

function _draw()
	if gamestate==0 then _draw_menu()
	elseif gamestate==1 then	_draw_game()	end
end

function _draw_menu()
	rectfill(0,0,127,127,0)
	sspr(0,8,72,16,28,28)
	print ("press z to continue!", 28,64,7)
end

function _draw_game()
	rectfill(0,0,127,127,0)
	drawfield()
	drawpaddles()
--	drawball()
--	drawpegs()
--	drawscore()
end

function drawfield()
	
end
